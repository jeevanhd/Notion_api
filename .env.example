# Notion Integration API Configuration
# Copy this file to .env and fill in your actual values

# Your Notion Integration Token
# Get this from: https://www.notion.so/my-integrations
# Example: secret_abc123def456ghi789
NOTION_TOKEN=your_notion_integration_token_here

# Your Notion Database ID  
# Get this from your database URL or share link
# Example: 1234567890abcdef1234567890abcdef
DATABASE_ID=your_notion_database_id_here

# Optional: API Configuration
# API_HOST=localhost
# API_PORT=8000
# DEBUG_MODE=True